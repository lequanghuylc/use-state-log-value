{
  "name": "use-state-log-value",
  "version": "0.2.0",
  "description": "State change logging utilities for frontend and backend",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./backend": {
      "types": "./dist/backend/index.d.ts",
      "default": "./dist/backend/index.js"
    }
  },
  "bin": {
    "stateLogServer": "dist/backend/cli.js",
    "use-state-log-value-install-rules": "scripts/postinstall.js"
  },
  "files": [
    "dist",
    "README.md",
    "use-state-log-value.mdc",
    "scripts"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "postinstall": "node scripts/postinstall.js",
    "test": "jest",
    "test:frontend": "jest --selectProjects frontend",
    "test:backend": "jest --selectProjects backend",
    "example:frontend": "vite --config examples/frontend/vite.config.ts",
    "example:todo": "cd examples/todo-frontend && npm install && npm run dev",
    "example:backend": "tsx examples/backend/demo.ts",
    "server": "tsx src/backend/cli.ts",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "keywords": [
    "react",
    "hook",
    "state",
    "logging"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "pg": "^8.14.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.19",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^14.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "start-server-and-test": "^2.0.10",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vite": "^6.2.0"
  }
}
